    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.2",
    "username-generator": "^1.1.0",
    "webpack": "^4.25.1",
